{
  "name": "leaflet-advanced-layer-system",
  "version": "1.0.1",
  "description": "An advanced layer system (and more) for Leaflet",
  "main": "dist/System.js",
  "browserslist": [
    "chrome >= 7",
    "firefox >= 4",
    "ie >= 8",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true
          },
          "targets": "chrome >= 7, firefox >= 4, ie >= 8, last 2 versions",
          "forceAllTransforms": true,
          "spec": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ],
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ]
    ]
  },
  "scripts": {
    "test": "node tests/runTests.js",
    "generateDocs": "jsdoc ./ -c jsdoc.conf.json",
    "prepublishOnly": "node build.js"
  },
  "keywords": [
    "leaflet",
    "leaflet-advanced-layer-system",
    "als",
    "layer-system",
    "advanced-layer-system",
    "plugin",
    "leaflet plugin",
    "leaflet layer system",
    "layer system",
    "advanced layer system"
  ],
  "author": "matafokka",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/matafokka/leaflet-advanced-layer-system",
  "repository": "https://github.com/matafokka/leaflet-advanced-layer-system",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "classlist": "github:eligrey/classList.js",
    "core-js": "^3.7.0",
    "debounce": "^1.2.1",
    "file-saver": "^2.0.5",
    "jscolor": "github:EastDesire/jscolor#2.1.1",
    "jszip": "^3.5.0",
    "keyboardevent-key-polyfill": "^1.1.0",
    "object-defineproperty-ie": "^3.1.0",
    "regenerator-runtime": "^0.13.7",
    "remixicon": "^2.5.0",
    "shpjs": "^3.6.2",
    "sortablejs": "~1.10.2",
    "time-input-polyfill": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.1",
    "babelify": "^10.0.0",
    "browserify": "~13.3.0",
    "chalk": "^2.4.2",
    "common-shakeify": "^0.6.2",
    "css-patch": "^1.1.2",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.0.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6",
    "jsdom": "^16.5.1",
    "jsdom-global": "^3.0.2",
    "leaflet": "^1.7.1",
    "postcss": "^8.2.4",
    "postcss-css-variables": "^0.17.0",
    "postcss-preset-env": "^6.7.0",
    "uglifyify": "^5.0.2"
  }
}
