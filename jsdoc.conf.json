{
  "plugins": [
	"plugins/markdown"
  ],
  "templates": {
	"systemName": "ALS",
	"theme": "cerulean",
	"linenums": true,
	"navType": "inline",
	"footer": "<div style='width: 100%; text-align: center;'><a href='https://github.com/matafokka'>(c) matafokka</a></div>",
	"monospaceLinks": true,
	"default": {
	  "useLongnameInNav": true
	}
  },
  "source": {
	"exclude": [
	  "./tests/",
	  "./_service/",
	  "./dist/",
	  "./build.js",
	  "./polyfills.js",
	  "./node_modules/"
	]
  },
  "opts": {
	"recurse": true,
	"destination": "./docs/",
	"template": "./node_modules/ink-docstrap/template",
	"readme": "./README_DOCS.md",
	"tutorials": "./jsdoc-tutorials"
  }
}